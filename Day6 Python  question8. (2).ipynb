        "# Reverse the order of elements in the fruits list using the reverse method..\n",
        "fruits=[\"apple\",\"banana\",\"grapes\",\"mango\",\"orange\"]\n",
        "numbers=[1,2,3]\n",
        "fruits.extend(numbers)\n",
        "print(fruits)\n",
        "fruits.index(\"orange\")\n",
        "fruits.reverse()\n",
        "print(fruits)\n",
        "\n"
