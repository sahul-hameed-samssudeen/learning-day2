        "year=int(input(\"enter the year you want check whether it is leap year or not :\"))\n",
        "if(year%4==0 and year%100!=0 or year%400==0):\n",
        "    print(\"The given year is\",year,\" leap year\")\n",
        "else:\n",
        "    print(\"The given year  \",year,\"is not the leap year\")\n"
